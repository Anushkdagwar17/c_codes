//Program to read the values of a,b,c and display value of x where x=a/b-c.

#include <stdio.h>

int main(){
   
    float a,b,c,x;
   
    printf("Enter the value of a: " );
    scanf("%f",&a);

    printf("Enter the value of b: " );
    scanf("%f",&b);

    printf("Enter the value of c: " );
    scanf("%f",&c);

    if(b==c)
        printf("Division by zero is not possible.\n" );
   
    else
        x=a/(b-c);
        printf("The value of x is: %.2f\n",x );
    

    return 0;
}
