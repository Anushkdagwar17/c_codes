// To display the multiplication table of number provided by the user.

#include <stdio.h>

int main() {
    int i,j,num;
    
    printf("Enter the number: ");
    scanf("%d",&num);
    
    printf("\nThe Multiplication Table from 1 to %d : \n\n",num);
    for(i=1;i<=num;i++){
        for(j=1;j<=10;j++){
            printf("%2d x %2d = %3d\n",j,i,j*i);
        }
        printf("\n");
    }

    return 0;
}
