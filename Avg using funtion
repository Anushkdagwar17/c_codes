#include<stdio.h>

float average(float num1,float num2,float num3){
    return (num1+num2+num3)/3;
    }
    
int main(){
    
    float num1,num2,num3;
    
    printf("Enter Num1,Num2,Num3: ");
    scanf("%f%f%f",&num1,&num2,&num3);
    float avg=average(num1, num2, num3);
    printf("Average= %.2f\n",avg);
    
    return 0;
    
}
    
